PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin

# Rotate the job log at midnight (keep same inode) on backup days
20 0 * * 2,5 /usr/bin/truncate -s 0 /var/log/rclone-backup.log

# Recycle VMs
20 4 * * * /bin/bash /usr/local/sbin/recycle-vm.sh 106 60
10 0 * * 6 /bin/bash /usr/local/sbin/recycle-vm.sh 105 60
15 0 * * 6 /bin/bash /usr/local/sbin/recycle-vm.sh 108 60
20 0 * * 6 /bin/bash /usr/local/sbin/recycle-vm.sh 107 60

# every thursday at 5:00 AM reload the VM 104
0 5 * * 4 /bin/bash /usr/local/sbin/recycle-vm.sh 104 60

# offside backup to S3 (megaS4)
20 2 * * 2,5 /bin/bash /usr/local/sbin/offsite_bckp.sh 104
20 3 * * 6 /bin/bash /usr/local/sbin/offsite_bckp.sh 101
20 4 * * 6 /bin/bash /usr/local/sbin/offsite_bckp.sh 102
20 5 * * 6 /bin/bash /usr/local/sbin/offsite_bckp.sh 103
20 6 * * 6 /bin/bash /usr/local/sbin/offsite_bckp.sh 109

# clear memory cache every 2 hours (10,12,14,16,18,etc)
0 */2 * * * echo 1 > /proc/sys/vm/drop_caches
